<h1>My People</h1>
  <p><%= link_to 'New Person', new_page_path %></p>
  <% @pages.each do |page| %>
    <h2><%= page.title %></h2>
    <%= link_to 'Show Person', page_path(page) %>
    <%= link_to 'Edit Person', edit_page_path(page) %>
    <%= link_to 'Destroy Person', page, method: :delete %>
<% end %>
